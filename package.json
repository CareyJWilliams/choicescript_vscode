{
	"name": "choicescript-vscode",
	"displayName": "ChoiceScript VSCode",
	"description": "ChoiceScript syntax highlighting and Intellisense",
	"version": "0.1.0",
	"publisher": "KLNeidecker",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Programming Languages"
	],
	"icon": "images/ChoiceOfGames.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:choicescript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "choicescript",
				"aliases": [
					"ChoiceScript",
					"choicescript",
					"cs"
				],
				"extensions": [
					".txt"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "choicescript",
				"scopeName": "source.cs",
				"path": "./syntaxes/cs.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "choicescript",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"__metadata": {
		"id": "7ea4fade-d4bd-40b0-a1b6-ff4e57a15079",
		"publisherDisplayName": "KLNeidecker",
		"publisherId": "f3bb5996-197b-4db3-8bc9-36a9d01defcc"
	}
}
